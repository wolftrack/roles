{
    "name": "wolfpack/roles",
    "description": "Roles and permissions for Laravel",
    "authors": [
        {
            "name": "Georg Thomassen",
            "email": "geo.cornelius@gmail.com",
            "homepage": "https://georgthomassen.dk",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Wolfpack\\Roles\\RolesServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Wolfpack\\Roles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wolfpack\\Roles\\Test\\": "tests/"
        }
    }
}
